<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.3" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd">
    <application>
        <locale-config>
            <default-locale>de_DE</default-locale>
            <supported-locale>de_DE</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>de.gedoplan.showcase.bpm.ui</base-name>
            <var>ui</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>start-issueManagement</from-outcome>
            <to-view-id>/issue/start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>stuff/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>stoerung-pruefen</from-outcome>
            <to-view-id>/issue/stoerung-pruefen.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>dashboard</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>stuff</from-outcome>
            <to-view-id>/stuff/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>orga</from-outcome>
            <to-view-id>/orga/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>